<ul>
  <%= form_for(@restaurant) do |f| %>
  <li><%= f.label :name %>
  <%= f.text_field :name %></li>

  <li><%= f.label :category %>
  <%= f.text_field :category %></li>

  <li><%= f.label :address %>
  <%= f.text_field :address %></li>

  <li><%= f.label :phone_number %>
  <%= f.text_field :phone_number %></li>

   <p><%= f.submit %></p>
  <% end %>
</ul>
